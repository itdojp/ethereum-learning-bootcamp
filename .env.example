# Copy this file to `.env` and fill in values.
# Never commit secrets (private keys, API keys).

# RPC URLs
SEPOLIA_RPC_URL=
MAINNET_RPC_URL=
OPTIMISM_RPC_URL=
POLYGON_ZKEVM_RPC_URL=

# Deployer private key (0x-prefixed hex)
PRIVATE_KEY=0x

# Block explorer API keys
# - Ethereum mainnet / Sepolia: Etherscan
ETHERSCAN_API_KEY=
# - Optimism: Optimistic Etherscan
OPTIMISTIC_ETHERSCAN_API_KEY=

# Optional (used by some scripts)
NFT_BASE=ipfs://<CID>/
NFT_ROYALTY_BPS=500

